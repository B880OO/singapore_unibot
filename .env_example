# Telegram Bot Token
TOKEN=your_telegram_bot_token_here

# PostgreSQL connection URL
DATABASE_URL=postgresql://username:password@host:port/dbname

# Fernet key for encrypting user credentials (use Fernet.generate_key())
FERNET_KEY=your_fernet_key_here

# List of Telegram admin user IDs (comma-separated)
ADMINS=[123456789,987654321,654321987]
